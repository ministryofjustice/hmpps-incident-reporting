{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "noEmit": true,
    "lib": ["dom", "dom.iterable", "es2020"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "typeRoots": [
      "../../node_modules/@types",
      "../../server/@types",
    ]
  },
  "exclude": [],
  "include": ["*.ts"],
}
